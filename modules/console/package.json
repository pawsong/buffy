  "main": "build/prod/server.js",
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
    "prepublish": "npm run install:link && npm run install:tsd && npm run build",
    "install:link": "node ../../tools/link",
    "install:tsd": "$npm_package_config_bin/tsd install",
    "clean": "$npm_package_config_bin/rimraf lib build",
