{
  "name": "@pasta/addon-code-editor",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pawsong/pasta.git"
  },
  "version": "0.0.0",
  "description": "",
  "start": "build/prod/server.js",
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
    "prepublish": "npm run install:link && npm run install:tsd && npm run build",
    "install:link": "node ../../tools/link",
    "install:tsd": "$npm_package_config_bin/tsd install",
    "install:jspm": "npm run clean:jspm && $npm_package_config_bin/jspm install && $npm_package_config_bin/jspm install --link npm:@pasta/core",
    "clean": "$npm_package_config_bin/rimraf build jspm_packages",
    "clean:build:prod": "$npm_package_config_bin/rimraf build/prod",
    "clean:jspm": "$npm_package_config_bin/rimraf jspm_packages",
    "build": "npm run clean:build:prod && $npm_package_config_bin/gulp build:prod",
    "test": "$npm_package_config_bin/gulp test",
    "test:watch": "$npm_package_config_bin/gulp test:watch",
    "serve": "$npm_package_config_bin/gulp serve:dev:watch",
    "start": "node $npm_package_start"
  },
  "files": [
    "build/prod"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.8.1",
    "babel-polyfill": "^6.3.14",
    "bluebird": "^3.0.5",
    "body-parser": "^1.14.1",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-jwt": "^3.3.0",
    "kerberos": "0.0.17",
    "lodash": "^3.10.1",
    "mongoose": "^4.3.6",
    "pkg-dir": "^1.0.0",
    "shortid": "^2.2.4",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.0",
    "eventsource-polyfill": "^0.9.6",
    "fbemitter": "^2.0.1",
    "http-proxy": "^1.13.1",
    "jquery.terminal": "^0.9.3",
    "material-ui": "^0.14.2",
    "radium": "^0.16.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-tap-event-plugin": "^0.2.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "links": [
    "../config",
    "../helper",
    "../core",
    "../components"
  ],
  "jspm": {
    "dependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "ts": "github:frankwallis/plugin-typescript@^2.5.6"
    },
    "devDependencies": {}
  }
}
