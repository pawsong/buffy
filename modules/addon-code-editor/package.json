{
  "name": "@pasta/addon-code-editor",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pawsong/pasta.git"
  },
  "version": "0.0.0",
  "description": "",
  "start": "build/prod/server.js",
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
    "prepublish": "npm run install:link && npm run install:jspm && npm run install:tsd && npm run build",
    "install:link": "node ../../tools/link",
    "install:tsd": "$npm_package_config_bin/tsd install",
    "install:jspm": "$npm_package_config_bin/jspm install && $npm_package_config_bin/jspm install --link npm:@pasta/core",
    "clean": "$npm_package_config_bin/rimraf build",
    "clean:prod": "$npm_package_config_bin/rimraf build/prod jspm_packages",
    "build": "npm run clean:prod && npm run install:jspm && $npm_package_config_bin/gulp build:prod",
    "test": "$npm_package_config_bin/gulp test",
    "test:watch": "$npm_package_config_bin/gulp test:watch",
    "serve": "$npm_package_config_bin/gulp serve:dev:watch",
    "start": "node $npm_package_start"
  },
  "files": [
    "build/prod",
    "jspm_packages"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.8.1",
    "babel-polyfill": "^6.3.14",
    "bluebird": "^3.0.5",
    "body-parser": "^1.14.1",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-jwt": "^3.3.0",
    "kerberos": "0.0.17",
    "lodash": "^3.10.1",
    "mongoose": "^4.3.6",
    "pkg-dir": "^1.0.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "fbemitter": "^2.0.1",
    "material-ui": "^0.14.2",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-tap-event-plugin": "^0.2.1"
  },
  "links": [
    "../config",
    "../helper",
    "../game-class",
    "../addon",
    "../core"
  ],
  "jspm": {
    "dependencies": {
      "ts": "github:frankwallis/plugin-typescript@^2.5.6",
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
