{
  "name": "@pasta/voxel-editor",
  "version": "1.0.0",
  "description": "Voxel Editor",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
    "prepublish": "npm run install:link && npm run build",
    "install:link": "node ../../tools/link",
    "install:tsd": "$npm_package_config_bin/tsd install",
    "clean": "$npm_package_config_bin/rimraf lib",
    "build": "npm run clean && $npm_package_config_bin/gulp build",
    "test": "$npm_package_config_bin/gulp test",
    "test:watch": "$npm_package_config_bin/gulp test:watch"
  },
  "license": "ISC",
  "dependencies": {
    "fbemitter": "^2.0.1",
    "flux": "^2.1.1",
    "get-pixels": "^3.2.3",
    "immutable": "^3.7.5",
    "ndarray": "^1.0.18",
    "ndarray-fill": "^1.0.1",
    "object-assign": "^4.0.1",
    "react-addons-update": "^0.14.5",
    "react-color": "^1.2.1",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "store": "^1.3.17",
    "three": "^0.73.0",
    "voxelize": "^1.0.0",
    "whatwg-fetch": "^0.10.1"
  },
  "peerDependencies": {
    "material-ui": "^0.13.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "redux": "^3.0.4",
    "bluebird": "^3.0.5"
  },
  "link": [
    "../helper-public",
    "../config-public"
  ]
}
