    "express": "^4.13.3",
    "frame-loop": "^1.2.1",
    "jsonwebtoken": "^5.4.1",
    "kerberos": "0.0.17",
    "lodash": "^3.10.1",
    "mongoose": "^4.3.6",
    "shortid": "^2.2.4",
    "socket.io": "^1.3.7"
