{
  "name": "@pasta/addon-voxel-editor",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pawsong/pasta.git"
  },
  "version": "1.0.0",
  "description": "Voxel Editor",
  "start": "build/prod/server.js",
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
    "prepublish": "npm run install:link && npm run install:tsd && npm run build",
    "install:link": "node ../../tools/link",
    "install:tsd": "$npm_package_config_bin/tsd install",
    "clean": "$npm_package_config_bin/rimraf build",
    "clean:prod": "$npm_package_config_bin/rimraf build/prod",
    "build": "npm run clean:prod && $npm_package_config_bin/gulp build:prod",
    "test": "$npm_package_config_bin/gulp test",
    "test:watch": "$npm_package_config_bin/gulp test:watch",
    "serve": "$npm_package_config_bin/gulp serve:dev:watch",
    "start": "node $npm_package_start"
  },
  "files": [
    "build/prod"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.8.1",
    "babel-polyfill": "^6.3.14",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "kerberos": "0.0.18",
    "mongoose": "^4.3.6",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "fbemitter": "^2.0.1",
    "immutable": "^3.7.6",
    "material-ui": "^0.14.2",
    "ndarray": "^1.0.18",
    "ndarray-fill": "^1.0.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.6",
    "react-addons-update": "^0.14.6",
    "react-color": "^1.3.4",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.5",
    "store": "^1.3.20",
    "three": "^0.73.2"
  },
  "links": [
    "../config",
    "../helper",
    "../core"
  ]
}
