{
  "name": "@pasta/admin",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pawsong/pasta.git"
  },
  "version": "0.0.0",
  "description": "",
  "main": "build/prod/server.js",
  "config": {
    "bin": "../../node_modules/.bin"
  },
  "scripts": {
    "prepublish": "npm run install:link && npm run install:tsd && npm run build",
    "install:link": "node ../../tools/link",
    "install:tsd": "$npm_package_config_bin/tsd install",
    "clean": "$npm_package_config_bin/rimraf lib build",
    "build": "npm run clean && $npm_package_config_bin/gulp build:prod",
    "test": "$npm_package_config_bin/gulp test",
    "test:watch": "$npm_package_config_bin/gulp test:watch",
    "serve": "$npm_package_config_bin/gulp serve:dev:watch",
    "start": "node $npm_package_main"
  },
  "files": [
    "build"
  ],
  "publicAssets": "build/prod/client/public/**",
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "express": "^4.13.3",
    "request": "^2.67.0",
    "source-map-support": "^0.4.0"
  },
  "link": [
    "../config-internal",
    "../config-public"
  ]
}
